{
    "name": "bic-engine/image",
    "description": "The library for reading, converting and decoding images",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "bic-engine/binary": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Bic\\Image\\": "src"
        }
    },
    "require-dev": {
        "vimeo/psalm": "^4.28",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload-dev": {
        "psr-4": {
            "Bic\\Image\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "platform-check": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
